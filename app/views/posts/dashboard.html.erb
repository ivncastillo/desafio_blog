<h1>Dashboard Administrador</h1>

<form method="get" action="/posts/create">
    
    <div class="form-group">
        <label for="title">Título</label>
        <input name="title"  class="form-control" placeholder="Ingresa título al post">
        
    </div>

    <div class="form-group">
        <label for="image">Imágen</label>
        <input name="image"  class="form-control" placeholder="Ingresar URL imágen">
    </div>

    <div class="form-group">
        <label for="content">Contenido</label>
        <textarea name="content" class="form-control" placeholder="Ingresar contenido post"> </textarea>
    </div>
    <button type="submit" class="btn btn-primary">Ingresar Post</button>
</form>
